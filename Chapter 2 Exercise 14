// Make a program that reads three char-type characters and then prints them a on each line. Use a single printf() command for this

#include<stdio.h>
#include<stdlib.h>

int main(){
    char x,y,z;
    scanf("%c %c %c",&x,&y,&z);

    printf("%c\n%c\n%c\n",x,y,z);

    system("pause");
    return 0;
}
